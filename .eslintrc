{
    "plugins": [
        "prettier",
        "@typescript-eslint"
    ],
    "extends": [
        "airbnb-typescript",
        "react-app",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "settings": {
        "import/resolver": {
            "typescript": {
                "alwaysTryTypes": true
            }
        }
    },
    "rules": {
        "react/destructuring-assignment": 0,
        "react/jsx-props-no-spreading": 0
    }
}